{
  "name": "Subway: Buenos Aires",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#303030",
    "editor.lineHighlightBackground": "#e0e0e0",
    "editor.selectionHighlightBackground": "#e0e0e0"
  },
  "tokenColors": [
    {
      "name": "Text",
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#303030"
      }
    },
    {
      "name": "Comments",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#d2d2d2"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation.definition.string, punctuation.definition.variable, punctuation.definition.string, punctuation.definition.parameters, punctuation.definition.string, punctuation.definition.array",
      "settings": {
        "foreground": "#303030"
      }
    },
    {
      "name": "Delimiters",
      "scope": "none",
      "settings": {
        "foreground": "#303030"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#303030"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword, keyword.control",
      "settings": {
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#ee1b2d"
      }
    },
    {
      "name": "Functions",
      "scope": "entity.name.function, meta.require, support.function.any-method",
      "settings": {
        "foreground": "#0467b3"
      }
    },
    {
      "name": "Classes",
      "scope": "meta.class, support.class, entity.name.class, entity.name.type.class",
      "settings": {
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Methods",
      "scope": "keyword.other.special-method",
      "settings": {
        "foreground": "#0467b3"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Support",
      "scope": "support.function",
      "settings": {
        "foreground": "#00aedb"
      }
    },
    {
      "name": "Strings, Inherited Class",
      "scope": "string, constant.other.symbol, entity.other.inherited-class",
      "settings": {
        "foreground": "#008066"
      }
    },
    {
      "name": "Integers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Floats",
      "scope": "none",
      "settings": {
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Boolean",
      "scope": "none",
      "settings": {
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#ee1b2d"
      }
    },
    {
      "name": "Attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Attribute IDs",
      "scope": "entity.other.attribute-name.id, punctuation.definition.entity",
      "settings": {
        "foreground": "#0467b3"
      }
    },
    {
      "name": "Selector",
      "scope": "meta.selector",
      "settings": {
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Values",
      "scope": "none",
      "settings": {
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Headings",
      "scope": "markup.heading punctuation.definition.heading, entity.name.section",
      "settings": {
        "fontStyle": "",
        "foreground": "#0467b3"
      }
    },
    {
      "name": "Units",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Bold",
      "scope": "markup.bold, punctuation.definition.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Italic",
      "scope": "markup.italic, punctuation.definition.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Code",
      "scope": "markup.raw.inline",
      "settings": {
        "foreground": "#008066"
      }
    },
    {
      "name": "Link Text",
      "scope": "string.other.link",
      "settings": {
        "foreground": "#ee1b2d"
      }
    },
    {
      "name": "Link Url",
      "scope": "meta.link",
      "settings": {
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#ee1b2d"
      }
    },
    {
      "name": "Quotes",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Separator",
      "scope": "meta.separator",
      "settings": {
        "background": "#e0e0e0",
        "foreground": "#303030"
      }
    },
    {
      "name": "Inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#008066"
      }
    },
    {
      "name": "Deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#ee1b2d"
      }
    },
    {
      "name": "Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#6c2181"
      }
    },
    {
      "name": "Colors",
      "scope": "constant.other.color",
      "settings": {
        "foreground": "#00aedb"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#00aedb"
      }
    },
    {
      "name": "Escape Characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#00aedb"
      }
    },
    {
      "name": "Embedded",
      "scope": "punctuation.section.embedded, variable.interpolation",
      "settings": {
        "foreground": "#fdba31"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid.illegal",
      "settings": {
        "background": "#ee1b2d",
        "foreground": "#ffffff"
      }
    },
    {
      "name": "String Variables",
      "scope": "string.variable",
      "settings": {
        "foreground": "#ee1b2d"
      }
    }
  ]
}
